%Tyler Matthews
%2/23/2019

R = 400;
L = 3e-3;
C = 10e-6;
S = 0.4;
Vin = 12;


A = [0, -1/L;
     1/C, -1/(R*C)];
C = [0, 1];
D = [0];

%PART A
%Vout / Vin
B = [S/L; 0]
[NUM,DEN] = ss2tf(A,B,C,D)
TF = tf(NUM,DEN)

%Vout / S
B = [Vin/L; 0]
[NUM,DEN] = ss2tf(A,B,C,D)
TF = tf(NUM,DEN)

POLES = pole(TF) %Only run this once because the poles do not change with a change in B
EIGENVALUES = eig(A)

%PART B
T = 1/55000;
z = 1000;
t = 0 : T/z:0.015;
